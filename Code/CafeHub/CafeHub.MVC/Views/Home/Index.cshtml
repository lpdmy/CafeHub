                                new { Id = 3, Title = "Birthday party", Description = "Celebrate in style with our tailored birthday event services." }
                            };
                        }

                        @foreach (var offer in offers)
                        {
                            <div class="accordion-item wow fadeInUp" data-wow-delay="@(0.2 * offer.Id)s">
                                <h2 class="accordion-header" id="offersheading@offer.Id">
                                    <button class="accordion-button @(offer.Id == 1 ? "" : "collapsed")"
                                            type="button"
                                            data-bs-toggle="collapse"
                                            data-bs-target="#offerscollapse@offer.Id"
                                            aria-expanded="@(offer.Id == 1 ? "true" : "false")"
                                            aria-controls="offerscollapse@offer.Id">
                                        @offer.Title
                                    </button>
                                </h2>
                                <div id="offerscollapse@offer.Id"
                                     class="accordion-collapse collapse @(offer.Id == 1 ? "show" : "")"
