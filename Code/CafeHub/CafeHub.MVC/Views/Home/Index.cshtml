    <!-- Interactive Process Layout Start -->
<div class="interactive interactive-process-layout">
    <div class="interactive-interactive-process-wrapper interactive-wrapper">
        <div class="interactive-con">
            <div class="interactive-con-inner interactive-grid">
                @{
                    var processItems = new[]
                    {
                        new { Index = 0, Title = "Full catering", Description = "Open for dining" },
                        new { Index = 1, Title = "Tea selection", Description = "Rituals for focus, health & slowness" },
                        new { Index = 2, Title = "Private dining", Description = "Book now" },
                        new { Index = 3, Title = "Laguna beach", Description = "Come's to diner now" }
                    };
                }

                @foreach (var item in processItems)
                {
                    <div class="interactive-process-item">
                        <div class="interactive-inner-process @(item.Index == 0 ? "activate" : "")" data-index="@item.Index">
                            <div class="process-content-wap">
                                <div class="process-inner-content-wap">
                                    <p>@item.Description</p>
                                    <h2><a href="#">@item.Title</a></h2>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>

            <!-- Interactive Process Image Start -->
            <div class="interactive-process-list-image">
                @for (int i = 1; i <= 4; i++)
                {
                    <div class="interactive-process-image img-@(i-1) @(i == 1 ? "show" : "")"
                         data-bg="@Url.Content("~/images/interactive-process-image-" + i + ".jpg")"
                         style="background-image: url('@Url.Content("~/images/interactive-process-image-" + i + ".jpg")');">
                    </div>
                }
            </div>
            <!-- Interactive Process Image End -->
        </div>
    </div>
</div>
<!-- Interactive Process Layout End -->

